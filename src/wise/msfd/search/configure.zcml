  <utility
    component=".vocabulary.a4_mru_reporting_cycle_factory"
    name="wise_search_a4_mru_reporting_cycle_factory"
    />

  <utility
    component=".vocabulary.a19_reporting_period"
    name="wise_search_reporting_period_art19"
    />

  <utility
    component=".vocabulary.a19_region_subregions"
    name="wise_search_region_subregions_art19"
    />

  <utility
    component=".vocabulary.a19_member_states"
