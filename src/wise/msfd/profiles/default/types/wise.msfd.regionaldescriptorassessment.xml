<?xml version="1.0" encoding="utf-8"?>
<object name="wise.msfd.regionaldescriptorassessment"
   meta_type="Dexterity FTI" i18n:domain="wise.msfd"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"
    i18n:translate="">Regional Descriptor Assessment</property>
 <property name="description"
    i18n:translate="">Region &gt; Descriptor &gt; Article &gt; Assessment </property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">wise.msfd.regionaldescriptorassessment</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++wise.msfd.regionaldescriptorassessment</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="wise.msfd.commentsfolder"/>
 </property>
 <property name="allow_discussion">True</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property
    name="klass">wise.msfd.compliance.content.RegionalDescriptorAssessment</property>
 <property name="behaviors">
  <element value="plone.app.lockingbehavior.behaviors.ILocking"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element
     value="plone.app.referenceablebehavior.referenceable.IReferenceable"/>
 </property>
 <property
    name="schema">wise.msfd.compliance.interfaces.IRegionalDescriptorAssessment</property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
