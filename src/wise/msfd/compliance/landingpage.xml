<landingpage>
    <year date="Date" extra-css-class="landpage-header sticky-row">
        <element who="Author" article="Related MSFD Article" task="Product/Report">
            <row colspan="country" get-method="get_header_countries" display-text="_country"/>
            <row colspan="region" get-method="get_header_regions" display-text="_region"/>
        </element>
    </year>
    <year date="2020">
        <element who="MS" article="17 (11)" task="2020 update of monitoring programme">
            <row colspan="country" get-method="" display-text="{} reports"/>
            <row colspan="region" get-method="" display-text="{} reports"/>
        </element>
        <element who="CIS" article="17 (11)" task="Update of monitoring programmes">
            <row colspan="row"
                 get-method="get_from_env_with_filename"
                 msfd-article="Article 17 (Art.11)"
                 report-type="CIS Guidance Document"/>
        </element>
    </year>

    <year date="2019">
        <element who="COM" article="20" task="1st report on implementation of the directive">
            <row colspan="row" get-method="get_from_env_2019_art20"
                 msfd-article="Article 20(1)" display-text=""/>
        </element>
        <element who="MS" article="12" task="MS response to recommendations">
            <row colspan="country" get-method="get_from_env_simple" display-text=""/>
        </element>
        <element who="COM" article="12" task="Recommendations to MS">
            <row colspan="country" get-method="get_from_env_simple" display-text="{} Recom." permission="wise.ViewAssessmentData"/>
        </element>
        <element who="COM" article="12" task="Assessment of 2018 Art. 8-9-10 updates">
            <row colspan="row" get-method="" display-text="View report" permission="wise.ViewAssessmentData"/>
            <row colspan="region" color-class="region-a" get-method="get_2018_regions_assess" display-text="View {} asses." permission="wise.ViewAssessmentData"/>
            <row colspan="country" color-class="country-a" get-method="get_2018_countries_assess" display-text="View {} asses." permission="wise.ViewAssessmentData"/>
        </element>
        <element who="MS" article="18" task="Interim report on programme of measures">
            <row colspan="country" get-method="get_from_env_simple" display-text="{} reports"/>
        </element>
        <element who="CIS" article="18" task="Interim report on programme of measures">
            <row colspan="row"
                 get-method="get_from_env_with_filename"
                 msfd-article="Article 18"
                 report-type="CIS Guidance Document"/>
        </element>
    </year>
    <year date="2018">
        <element who="MS" article="17 (8, 9, 10)" task="2018 update of assessment, determination of GES, environmental targets">
            <row colspan="country" get-method="get_2018_countries_reports" display-text="{} reports"/>
            <row colspan="region" get-method="get_2018_regions_reports" display-text="{} reports"/>
        </element>
        <element who="CIS" article="17 (8, 9, 10)" task="Update of assessment, GES determination and environmental targets">
            <row colspan="row" get-method="get_from_env_multiple_cis"
                 msfd-article="Article 17 (Art.8-9-10)"/>
        </element>
    </year>

    <year date="2016">
        <element who="MS" article="16" task="MS response to recommendations">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 16 (Art.13-14)"
                 report-type="Member State response to Commission recommendations"
                 display-text="{} response"/>
        </element>
        <element who="COM" article="16" task="Recommendations to MS">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 16 (Art.13-14)"
                 report-type="Commission recommendations to Member State"
                 display-text="{} letter"/>
        </element>
        <element who="COM" article="16" task="Assessment of 2016 Art. 13-14 reports">
            <row colspan="row" get-method="get_from_env_2016_art16" display-text=""
                 msfd-article="Article 16 (Art.13-14)"/>
            <row colspan="region" get-method="get_from_env_simple"
                 msfd-article="Article 16 (Art.13-14)"
                 report-type="Commission technical assessment - regional"
                 display-text="{} technical report" color-class="region-a"/>
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 16 (Art.13-14)"
                 report-type="Commission technical assessment - national"
                 display-text="{} asses." color-class="country-a"/>
        </element>
        <element who="COM" article="9.3" task="Criteria and methodological standards for GES (2017 revision)">
            <row colspan="row" get-method="get_from_env_2016_art9_3" display-text=""
                 msfd-article="Article 9(3)"/>
        </element>
        <element who="MS" article="13, 14" task="Programme of measures, exceptions">
            <row colspan="country" get-method="" display-text="{} reports"/>
            <row colspan="region" get-method="" display-text="{} reports"/>
        </element>
        <element who="CIS" article="13, 14" task="Programmes of measures and exceptions">
            <row colspan="row" get-method="get_from_env_multiple_cis"
                 msfd-article="Article 13-14"/>
        </element>
    </year>

    <year date="2015">
        <element who="MS" article="12" task="MS response to recommendations">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.11)"
                 report-type="Member State response to Commission recommendations"/>
        </element>
        <element who="COM" article="12" task="Recommendations to MS">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.11)"
                 report-type="Commission recommendations to Member State"
                 display-text="{} letter"/>
        </element>
        <element who="COM" article="12" task="Assessment of 2014 Art. 11 reports">
            <row colspan="row" get-method="get_from_env_2015_art12"
                 msfd-article="Article 12 (Art.11)"
                 display-text=""/>
            <row colspan="region" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.11)"
                 report-type="Commission technical assessment - regional"
                 display-text="{} technical report"
                 color-class="region-a"/>
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.11)"
                 report-type="Commission technical assessment - national"
                 display-text="{} report"
                 color-class="country-a"/>
        </element>
        <element who="CIS" article="15" task="Community action">
            <row colspan="row"
                 get-method="get_from_env_with_filename"
                 msfd-article="Article 15"
                 report-type="CIS Guidance Document"/>
        </element>
    </year>

    <year date="2014">
        <element who="COM" article="21" task="Protected areas">
            <row colspan="row" get-method="get_from_env_with_filename"
                 msfd-article="Article 21"
                 report-type="Commission report"/>
        </element>
        <element who="MS" article="11" task="Monitoring programme">
            <row colspan="country" get-method="get_2018_countries_reports" display-text="{} Reports"/>
            <row colspan="region" get-method="get_2018_regions_reports" display-text="{} Reports"/>
        </element>
        <element who="CIS" article="11" task="Programmes of measures and exceptions">
            <row colspan="row" get-method="get_from_env_multiple_cis"
                 msfd-article="Article 13-14"/>
        </element>
    </year>

    <year date="2013">
        <element who="MS" article="12" task="MS response to recommendations">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.8-9-10)"
                 report-type="Member State response to Commission recommendations"
                 display-text="{} response"/>
        </element>
        <element who="COM" article="12" task="Recommendations to MS">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.8-9-10)"
                 report-type="Commission recommendations to Member State"
                 display-text="{} letter"/>
        </element>
        <element who="COM" article="12" task="Assessment of 2012 Art. 8-9-10 reports">
            <row colspan="row" get-method="get_from_env_2012_art12"
                 msfd-article="Article 12 (Art.8-9-10)"
                 display-text=""/>
            <row colspan="region" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.8-9-10)"
                 report-type="Commission technical assessment - regional"
                 display-text="{} technical report" color-class="region-a"/>
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 12 (Art.8-9-10)"
                 report-type="Commission technical assessment - national"
                 display-text="{} Report" color-class="country-a"/>
        </element>
    </year>

    <year date="2012">
        <element who="MS" article="8, 9, 10" task="Initial assessment, determination of GES, environmental targets">
            <row colspan="country" get-method="get_2018_countries_reports" display-text="{} reports"/>
            <row colspan="region" get-method="get_2018_regions_reports" display-text="{} reports"/>
        </element>
        <element who="CIS" article="8, 9, 10" task="Initial assessment, determination of GES and environmental targets">
            <row colspan="row" get-method="get_from_env_multiple_cis"
                 msfd-article="Article 8-9-10"/>
        </element>
        <element who="COM" article="20.2" task="International report">
            <row colspan="row" get-method="get_from_env_with_filename"
                 msfd-article="Article 20(2)"
                 report-type="Commission report"/>
        </element>
    </year>

    <year date="2011">
        <element who="MS" article="7" task="Competent authorities">
            <row colspan="country" get-method="get_2018_countries_reports"
                 display-text="{} reports"/>
        </element>
        <element who="CIS" article="7" task="Competent authorities">
            <row colspan="row"
                 get-method="get_from_env_with_filename"
                 msfd-article="Article 7"
                 report-type="CIS Guidance Document"/>
        </element>
    </year>

    <year date="2010">
        <element who="MS" article="4" task="Subdivisions of marine regions or subregions">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 4"
                 report-type="Member State notification to Commission of subdivisions"
                 display-text="{} reports"/>
        </element>
        <element who="COM" article="9.3" task="Criteria and methodological standards for GES">
            <row colspan="row" get-method="get_from_env_2010_9_3"
                 msfd-article="Article 9(3)"/>
        </element>
        <element who="MS" article="26" task="Transposition of directive">
            <row colspan="country" get-method="get_from_env_simple"
                 msfd-article="Article 4"
                 report-type="Member State letter to Commission on transposition"
                 display-text="{}"/>
        </element>
    </year>

    <year date="2008">
        <element who="COM" article="-" task="Marine Strategy Framework Directive">
            <row colspan="row" get-method="get_from_env_with_filename"
                 msfd-article="MSFD"
                 report-type="Commission directive"/>
