        <element who="MS" article="12" task="MS response to recommendations">
            <row colspan="country" get-method="" display-text=""/>
        </element>
        <element who="COM" article="12" task="Recommendations to MS">
            <row colspan="country" get-method="get_2018_countries_reports" display-text="View {} Recom." permission="wise.ViewAssessmentData"/>
        </element>
        <element who="COM" article="12" task="Assessment of 2018 updated Art8-9-10 reports">
