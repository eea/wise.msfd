<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  lang="en"
  metal:use-macro="context/main_template/macros/master"
  i18n:domain="eea">

  <head>
    <metal:block fill-slot="style_slot">
      <link href="/marine/++resource++static_css/tabs.css" type="text/css" rel="stylesheet" />
      <link href="/marine/++resource++static_css/msfd_search.css" type="text/css" rel="stylesheet" />
    </metal:block>
  </head>

  <body metal:fill-slot="main">
    <div id="layout-contents" tal:define="text context/text | nothing" >
      <div id="wise-search-form">
        <div id="wise-search-form-top">
          <div tal:condition="text" tal:content="structure text/output"></div>

          <div class="topnav articles-wrapper row">

             <a class="article-box-anchor"
                tal:define="_href python:'/marine/assessment-module/regional-summaries/' + context._rcode + '/overview'"
                tal:attributes="href _href">
               <div class="article-box col-lg-4 col-xs-6">
                 <div class="article-box-wrapper">
                   <div class="article-box-header">
                     <h4 class="tab-subtitle">Overview</h4>
                     <div class="tab-title"
                          tal:content="string: Overview for ${view/context/title}">Overview for MS
                     </div>
                   </div>
                   <div class="article-description">
                     Longer introductory text or description here ...
                   </div>
                   <a href=""
                      tal:attributes="href _href"
                      class="article-link"
                      target="_blank">
                     <span class="link-btn"></span>
                   </a>
                 </div>
               </div>
             </a>

             <a class="article-box-anchor"
                tal:define="_href python:'/marine/assessment-module/regional-descriptors-assessments/' + context._rcode + '/reports'"
                tal:attributes="href _href">
               <div class="article-box col-lg-4 col-xs-6">
                 <div class="article-box-wrapper">
                   <div class="article-box-header">
                     <h4 class="tab-subtitle">Reports
                     </h4>
                     <div class="tab-title"
                          tal:content="string: View reports for ${view/context/title}">View reports by Member State
                     </div>
                   </div>
                   <div class="article-description">
                     Longer introductory text or description here ...
                   </div>
                   <a href=""
                      tal:attributes="href _href"
                      class="article-link"
                      target="_blank">
                     <span class="link-btn"></span>
                   </a>
                 </div>
               </div>
             </a>

             <a class="article-box-anchor"
                tal:define="_href python:'/marine/assessment-module/regional-descriptors-assessments/' + context._rcode + '/assessments'"
                tal:attributes="href _href">
               <div class="article-box col-lg-4 col-xs-6">
                 <div class="article-box-wrapper">
                   <div class="article-box-header">
                     <h4 class="tab-subtitle">Assessments
                     </h4>
                     <div class="tab-title">View or edit the ongoing assessments
                     </div>
                   </div>
                   <div class="article-description">
                     Longer introductory text or description here ...
                   </div>
                   <a href=""
                      tal:attributes="href _href"
                      class="article-link"
                      target="_blank">
                     <span class="link-btn"></span>
                   </a>
                 </div>
               </div>
             </a>
          </div>
        </div>
      </div>
    </div>

    <metal:css fill-slot="formtop">
      <script src="++resource++msfd/js/msfd_search.js"></script>
    </metal:css>

  </body>
</html>
