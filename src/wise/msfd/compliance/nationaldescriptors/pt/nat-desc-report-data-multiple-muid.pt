<div class="overflow-table"
  tal:define="title options/title; data options/data">
  <h5 tal:content="title">2018 Member State Report</h5>
  <div tal:condition="data" tal:repeat="info data">
    <div
      tal:define="muid python:info[0]; muid_data python: info[1];"
      >
      <h3 tal:content="muid">LV-01</h3>
      <table class="table table-bordered table-striped"
        >
        <tr tal:repeat="row muid_data">
          <th tal:content="python: row[0]">Element</th>
          <td tal:repeat="r python: row[1]">
            <span tal:content="r">Value</span>
          </td>
        </tr>
      </table>
    </div>
  </div>
</div>
