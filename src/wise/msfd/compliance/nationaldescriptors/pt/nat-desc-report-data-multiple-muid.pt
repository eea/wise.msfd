<div tal:define="data options/data">

  <!-- TODO: re&#45;enable the harvest subform -->
  <div class="subform-refresh" tal:define="form nocall:view/subform" tal:replace="nothing">
    <div tal:replace="structure form">
      refresh
    </div>
  </div>

  <div tal:condition="not: data">
    No data reported for this Descriptor
  </div>

  <div style="clear: both"><!-- -->
  </div>

  <div tal:condition="data" tal:repeat="info data" class="report-section">

    <tal:def tal:define="muid python:info[0]; muid_data python: info[1];" >
      <h4 tal:content="muid">LV-01</h4>
      <div class="overflow-table">
        <table class="table table-bordered table-striped table-comp-translate">
          <tal:rep tal:repeat="row muid_data">
            <tr tal:condition="python: row[0] not in view.BLACKLIST">
              <th tal:content="python: row[0]">Element</th>
              <tal:rep tal:repeat="r python: row[1]">
                <td tal:replace="structure python: view.translate_value(value=r)"> </td>
              </tal:rep>
            </tr>
          </tal:rep>
        </table>
      </div>
    </tal:def>

  </div>
</div>
