<div class="overflow-table"
  tal:define="data options/data">

  <!-- <div tal:define="form nocall:view/subform"
    style="padding: 1em; margin: 1em 0 1em 1em; background-color: #CCC; width: 20em; float: right"> -->
  <div class="subform-refresh" tal:define="form nocall:view/subform">
    <div tal:replace="structure form">
      refresh
    </div>
  </div>

  <!-- <div style="overflow: auto" tal:content="structure options/head_tpl">Header</div> -->
  <div class="head-tpl" tal:content="structure options/head_tpl">Header</div>

  <div tal:condition="not: data">
    No data reported for this Descriptor
  </div>

  <div style="clear: both; overflow:hidden"><!-- --></div>

  <tal:def tal:condition="data"
     tal:define="muid_data python: data[0][1];
     muids python:view.get_muids_from_data(data)">
    <div>
      <!--<h4 tal:content="muids">LV-01</h4>-->
      <table class="table table-bordered table-striped">
        <tr>
          <th>MarineReportingUnit</th>
          <td colspan="50" tal:content="muids">LV-001, LV-002</td>
        </tr>
        <tal:rep tal:repeat="row muid_data">
          <tr tal:condition="python: row[0] not in view.BLACKLIST">
            <th tal:content="python: row[0]">Element</th>
            <td tal:repeat="r python: row[1]">
              <!-- <metal:m use&#45;macro="context/@@wise&#45;macros/macros/translate"></metal:m> -->
              <div tal:replace="structure python: view.translate_value(value=r)"></div>
            </td>
          </tr>
        </tal:rep>
      </table>
    </div>
  </tal:def>
</div>
