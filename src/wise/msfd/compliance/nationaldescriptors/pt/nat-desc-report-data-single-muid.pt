<div tal:define="data options/data">

  <!-- TODO: re&#45;enable the harvest subform -->
  <div class="subform-refresh" tal:replace="nothing">
    <div tal:replace="structure view/subform">
      refresh
    </div>
  </div>

  <div tal:condition="not: data">
    No data reported for this Descriptor
  </div>

  <div style="clear: both; overflow:hidden"><!-- --></div>

    <tal:cond tal:condition="data">
      <tal:def
        tal:define="muid_data python: data[0][1];
        muids python:view.get_muids_from_data(data)"
        >
        <div class="overflow-table">
          <!--<h4 tal:content="muids">LV-01</h4>-->
          <table class="table table-bordered table-striped table-comp-translate">
            <tr>
              <th>MarineReportingUnit</th>
              <td colspan="50" tal:content="muids">LV-001, LV-002</td>
            </tr>
            <tal:rep tal:repeat="row muid_data">
              <tr tal:condition="python: row[0] not in view.BLACKLIST">
                <th tal:content="python: row[0]">Element</th>
                <td tal:repeat="r python: row[1]">
                  <div tal:content="r"></div>
                  <!-- <div tal:replace="structure python: view.translate_value(value=r)"></div> -->
                </td>
              </tr>
            </tal:rep>
          </table>
        </div>
      </tal:def>
    </tal:cond>

</div>
