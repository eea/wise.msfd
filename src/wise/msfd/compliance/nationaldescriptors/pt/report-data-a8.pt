<div>
  <div tal:repeat="muid view/muids" class="report-section">
    <tal:def define="data view/rows/?muid">

      <h4 tal:content="muid">DE_ANS</h4>

      <metal:s
        metal:use-macro="context/context/wise-macros/macros/simplify-table"></metal:s>

      <div class="overflow-table">
        <div class="inner">
          <table tal:condition="data"
            class='table table-bordered table-striped table-report'>
            <tal:rep tal:repeat="row data" >
              <tal:row replace="structure row"></tal:row>
            </tal:rep>
          </table>
        </div>
      </div>

    </tal:def>
  </div>
</div>
