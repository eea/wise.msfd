<div class="row">
  <div class="col-md-12">
    <h4 tal:content="options/title">201X Member State Report</h4>
    <a href="./">Back to Descriptor</a>
    <table class="table table-bordered table-striped first-table">
      <tr>
        <td>Member State</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="python:view.country_name"/>
      </tr>
      <tr>
        <td>Descriptor</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="python:view.desc_label"/>
      </tr>
      <tr>
        <td>Region(s) and subregion(s)</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="python:', '.join(view.regions)"/>
      </tr>
      <tr>
        <td>Article reported</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="view/article"/>
      </tr>
      <tr>
        <td>Reported by</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="options/report_by"/>
      </tr>
      <tr>
        <td>Source file of Member State</td>
        <td tal:attributes="colspan view/colspan">
          <a tal:attributes="href options/source_file"
           tal:content="options/source_file"/>
        </td>
      </tr>
      <tr>
        <td>Date report due</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="options/report_due"/>
      </tr>
      <tr>
        <td>Date reported by Member State</td>
        <td tal:attributes="colspan view/colspan"
           tal:content="options/report_date"/>
      </tr>
    </table>
  </div>
</div>