<div id="edit-history" tal:on-error="string: Edit History is empty"
                        tal:define="records_table view/report_assessment">
    <h2 tal:content="structure view/title">Edit History</h2>
    <table class="table table-listing" style="width:100%; margin-bottom:5rem; border: solid black;"
            tal:define="timestamps records_table/timestamps">
        <th>Fields / Records</th>
        <th tal:repeat="timestamp timestamps"
            tal:content="timestamp"></th>
        <tr tal:repeat="field python: records_table.keys()">
            <td tal:content="field"></td>
            <tal:rep tal:repeat="value python: records_table[field]">
                <td tal:content="value"></td>
            </tal:rep>
        </tr>
    </table>
</div>
